{%  extends "::base.html.twig" %}

{% block title %} 
Réservations
{% endblock %}

{% block body %}
<h3>
    Les réservations arrivées :
</h3>
    {% if reservations is defined %}
    <ul>
    {% for reservation in reservations %}
            <li>
                {{ reservation }}
            </li>
    {% endfor %}

    </ul>
        {% else %}
        <span>
            Personne n'est encore arrivé !
        </span>
        {% endif %}
{% endblock %}